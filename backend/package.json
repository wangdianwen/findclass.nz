{
  "name": "findclass.nz-backend",
  "version": "1.0.0",
  "description": "findclass.nz - Course Search Platform Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "ts-node scripts/generate-openapi.ts && tsc --project tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "vitest run tests/unit",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check": "npm run lint && npm run format && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "lambda:build": "esbuild src/lambda/index.ts --bundle --platform=node --outfile=dist/lambda/bundle.js --format=esm"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-lambda": "^3.450.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/client-ses": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.12",
    "p-retry": "^7.1.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^7.0.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "axios": "^1.13.2",
    "chokidar": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "js-yaml": "^4.1.1",
    "prettier": "^3.1.1",
    "supertest": "^7.2.2",
    "testcontainers": "^11.11.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "findclass.nz Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/findclassnz/findclass.nz"
  },
  "keywords": [
    "findclass.nz",
    "findclass",
    "education",
    "course-search",
    "new-zealand"
  ],
  "license": "MIT"
}
